{
  "expo": {
    "name": "slawn-shinto",
    "slug": "slawn-shinto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/slawn_logo_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/slawn_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/slawn_adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.slawnshinto"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "27dcb328-0d3a-4f5c-8909-ae06e5eac8de"
      }
    }
  }
}
